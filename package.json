{
  "name": "vue3-vtable",
  "version": "0.0.1",
  "description": "Lightweight virtual list component for vue 3",
  "main": "dist/vue-vtable.umd.js",
  "module": "dist/vue-vtable.esm.js",
  "types": "dist/index.d.ts",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-vtable.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vue-vtable.esm.js",
    "build": "npm-run-all build:*",
    "make-example": "webpack --config ./webpack.config.example.js --mode production",
    "dev": "webpack-cli serve --config ./webpack.config.example.js --mode development",
    "eslint": "eslint \"{src,example/src}/**/*.{ts,tsx}\"",
    "eslint:fix": "eslint \"{src,example/src}/**/*.{ts,tsx}\" --fix",
    "prettier": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --list-different",
    "prettier:fix": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --write"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/codemirror": "^0.0.98",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-loader": "^8.1.0",
    "codemirror": "^5.58.1",
    "css-loader": "^4.3.0",
    "emotion": "^10.0.27",
    "eslint": "^7.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "rollup": "^2.29.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "vue": "^3.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1"
  }
}
