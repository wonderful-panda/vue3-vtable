{
  "name": "vue3-vtable",
  "version": "0.0.1",
  "description": "Lightweight virtual list component for vue 3",
  "main": "dist/vue-vtable.umd.js",
  "module": "dist/vue-vtable.esm.js",
  "types": "dist/index.d.ts",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-vtable.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vue-vtable.esm.js",
    "build": "npm-run-all build:*",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "prettier": "prettier \"src/**/*.{ts,tsx}\" --list-different",
    "prettier:fix": "prettier \"src/**/*.{ts,tsx}\" --write"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rollup": "^2.29.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "typescript": "^4.0.3",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1"
  }
}
